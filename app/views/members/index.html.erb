<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name&nbsp;&nbsp;&nbsp;</th>
      <th>Born&nbsp;&nbsp;&nbsp;</th>
      <th>Gender&nbsp;&nbsp;&nbsp;</th>
      <th>Key&nbsp;&nbsp;&nbsp;</th>
      <th>FAMC&nbsp;&nbsp;&nbsp;</th>
      <th>FAMS&nbsp;&nbsp;&nbsp;</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.display_name %></td>
        <td><%= member.birth.try :date_string %></td>
        <td><%= member.gender %></td>
        <td><%= member.key %></td>
        <td><%= member.famc_key %></td>
        <td><%= member.fams_keys %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
